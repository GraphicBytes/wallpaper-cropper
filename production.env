COMPOSE_PROJECT_NAME=wallpapercropper-v2-web

DOCUMENT_ROOT=www

PHP_INI=.docker/host/php/php.ini
APACHE_PREFORK=.docker/host/apache/mpm_prefork.conf

HOST_NAME=wallpapercropper.com
HOST_CONTAINER_NAME=wallpapercropper-v2-web
HOST_CONTAINER_IMAGE=php:wallpapercropper-v2-web
HOST_PORT=1044

DB_CONTAINER_NAME=wallpapercropper-v2-db
DB_CONTAINER_IMAGE=mariadb:wallpapercropper-v2-web
DB_PORT=1046

DBADMIN_CONTAINER_NAME=wallpapercropper-v2-dbadmin
DBDBADMIN_PORT=1045

